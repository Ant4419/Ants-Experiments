# Example environment variables for Ants-Experiments
# Copy this file to .env and fill in your values

# EVM/Anvil
ANVIL_RPC=http://localhost:8545
CHAIN_ID=31337
METAMASK_PRIVATE_KEY=0xabc...

# CEX/DEX
CEX_PORT=8000
DEX_PORT=9000
DASHBOARD_PORT=3001

# CEX
CEX_API_PORT=8000
CEX_API_KEY=changeme-key
CEX_API_SECRET=changeme-secret
CEX_DB_PATH=/app/cex/cex.db

# Prometheus & Grafana
PROMETHEUS_PORT=9090
GRAFANA_PORT=3000
GF_SECURITY_ADMIN_PASSWORD=admin

# Add any other service-specific variables below
